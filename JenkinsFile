pipeline {
    agent any

    stages {
        stage('Clone Infra Repo') {
            steps {
                git branch: 'main',
                    url: 'https://github.com/Rishika10442/spe-microservice.git',
                    credentialsId: 'github-token'
            }
        }

        stage('Verify Checkout') {
            steps {
                sh 'ls -la'
            }
        }
    }
}
